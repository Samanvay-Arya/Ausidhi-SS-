<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.docsapp" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission
        android:name="android.permission.CALL_PHONE"
        android:required="true" />
    <uses-permission
        android:name="android.permission.READ_CONTACTS"
        android:required="true" />
    <uses-permission
        android:name="android.permission.SEND_SMS"
        android:required="true" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission
        android:name="android.permission.CAMERA"
        android:required="true" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        tools:ignore="ScopedStorage" />

    <queries>
        <intent>
            <action android:name="android.media.action.IMAGE_CAPTURE" />
        </intent>
    </queries>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_full"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_full"
        android:supportsRtl="true"
        android:theme="@style/Theme.DocsApp"
        tools:ignore="ExtraText" >
        <activity android:name=".MainActivity2" >
        </activity>
        <activity android:name=".AllProductUniversal" />
        <activity android:name=".IndividualProductUniversal" />
        <activity android:name=".OrderForLabTest" />
        <activity android:name=".Individual_Notification" />
        <activity android:name=".YourAccount" />
        <activity android:name=".LoginWithEmail" />
        <activity android:name=".RegisterWithEmail" />
        <activity android:name=".SignUp" />
        <activity android:name=".FirstLogin" />
        <activity android:name=".Logo_launcher" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login_2" />
        <activity android:name=".Login_1" />
        <activity android:name=".AllNotificationsActivity" />
        <activity android:name=".OrdersList" />
        <activity android:name=".Cart" />
        <activity android:name=".gadgets_first_page" />
        <activity android:name=".Brands" />
        <activity android:name=".search_by_category" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Universal_Search" />
        <activity android:name=".DetailUniversal" />
        <activity android:name=".OffersUniversal" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>